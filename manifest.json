{
"manifest_version": 2,
"name": "请求重定向",
"version": "0.0.1",

"browser_action":{
},

"background": {
    "scripts": ["background.js"]
},

"permissions": [
    "activeTab",
    "cookies",
    "tabs",
    "storage",
    "webRequest", 
    "webRequestBlocking", 
    "<all_urls>",

    "system.cpu"
 ],


"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",

"web_accessible_resources": [
]

}
